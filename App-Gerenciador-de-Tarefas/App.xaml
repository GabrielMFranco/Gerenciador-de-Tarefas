<Application x:Class="App_Gerenciador_de_Tarefas.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:c="clr-namespace:App_Gerenciador_de_Tarefas.Converters"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:App_Gerenciador_de_Tarefas"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
		<ResourceDictionary>
			<c:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
			<c:DueDateToBrushConverter x:Key="DueDateToBrushConverter"/>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="Styles/Button.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
